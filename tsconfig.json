{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@src": ["./src"],
      "@components": ["./src/components"],
      "@config": ["./src/config"],
      "@contexts": ["./src/contexts"],
      "@assets/*": ["./src/assets/*"],
      "@screens": ["./src/screens"],
      "@models/*": ["./src/models/*"],
      "@utils/*": ["./src/utils/*"],
      "@services": ["./src/services"]
    }
  }
}
